{
  "family": "railwiseTask",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "arn:aws:iam::940482421209:role/railwiseExecutionRole",
  "cpu": "256",               
  "memory": "512",             
  "containerDefinitions": [
    {
      "name": "railwiseBackendContainer",
      "image": "REPOSITORY_URI:IMAGE_TAG",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
